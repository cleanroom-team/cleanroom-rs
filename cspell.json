{
  "version": "0.2",
  "language": "en",
  "languageSettings": [
    {
      "languageId": "rust",
      "words": ["canonicalize", "chrono", "mktemp", "thiserror"],
      "ignoreRegExpList": [
        "/#\\[cfg\\b.+\\]/",
        "/#\\[repr\\b.+\\]/",
        "/(\\b|::)[a-z0-9_]+::/",
        "/::[a-z0-9_]+(\\b|::)/"
      ]
    },
    {
      "languageId": "toml",
      "words": ["chrono", "mktemp", "serde", "thiserror"]
    },
    {
      "languageId": "markdown",
      "words": [],
      "ignoreRegExpList": ["`[^`]+`"]
    }
  ],
  "words": [
    "cleanroom",
    "clrm",
    "cmdline",
    "envrc",
    "nspawn",
    "rustc",
    "rustfmt",
    "serpentos",
    "setenv",
    "tmpfs"
  ],
  "ignorePaths": [
    ".github/actions/*/*.yaml",
    ".github/workflows/*.yaml",
    "crates/cli/commands",
    "crates/cli/src/*.sh",
    "cspell.json"
  ]
}
